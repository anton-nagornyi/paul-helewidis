{
  "extends": ["airbnb-typescript/base"],
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "no-plusplus": "off",
    "no-restricted-syntax": "off",
    "max-len": "off",
    "no-mixed-operators": "off"
  }
}
